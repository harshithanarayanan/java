import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s = new Scanner(System.in);
        int a = s.nextInt();
        int b = s.nextInt();
        char c =s.next().charAt(0);
        switch(c)
        {
        case '+':
        float a1 = (float)a+b;
        System.out.println("Addition of two number is "+a1);
        break;
        case '-':
        float a2 = (float)a-b;
        System.out.println("Subtraction of two number is "+a2);
        break;
        case '*':
        float a3 = (float)a*b;
        System.out.println("Multiplication of two number is "+a3);
        break;
        case '/':
        float a4 = (float)a/b;
        System.out.println("Division of two number is "+a4);
        break;
        case '%':
        float a5 = (float)a%b;
        System.out.println("Modulo of two number is "+a5);
        break;
        default: 
        System.out.println("Invalid Input");
                
        }
           
        /* Enter your code here. Read input from STDIN. Print output to STDOUT. Your class should be named Solution. */
    }
}
